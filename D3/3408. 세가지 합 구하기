#include <iostream>
#include <vector>
#include <string>
using namespace std;

/*
20.05.07
3408. 세가지 합 구하기
그냥 수학 공식.
*/

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);

	int tc;
	cin >> tc;
	for (int t = 1; t <= tc; t++) {
		long n;
		cin >> n;
				
		cout << "#" << t << " " << n*(n+1)/2<<" "<<n*n<<" "<<n*(n+1) << '\n';
	}

	return 0;
}
