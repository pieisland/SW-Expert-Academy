#include<iostream>
#include<string>
#include <math.h>
using namespace std;

/*
200420
6718. 희성이의 원근법
그냥 endl 쓰지 말라는 문제.
*/

int main(void) {

	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);

	int tc;
	cin >> tc;
	for (int t = 1; t <= tc; t++) {
		int n;
		int ans;
		cin >> n;
		if (n < 100) ans = 0;
		else if (n < 1000) ans = 1;
		else if (n < 10000) ans = 2;
		else if (n < 100000) ans = 3;
		else if (n < 1000000) ans = 4;
		else ans = 5;

		cout << "#" << t<<" " << ans << '\n';
	}

	return 0;
}
