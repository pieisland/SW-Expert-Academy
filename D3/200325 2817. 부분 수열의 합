#include<cstdio>
#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<cstring>
#include<queue>
#include<math.h>
using namespace std;

/*
200325
2817. 부분 수열의 합
기본 dfs 문제.
*/

int n, k;
int arr[20];
int ans;

//중복선택이 안되는거 맞나?
void dfs(int start, int mySum) {
	if (mySum == k) {
		ans++;
		return;
	}
	if (start >= n) {
		return;
	}

	for (int i = start; i < n; i++) {
		dfs(i + 1, mySum + arr[i]);
	}
}

void clearArr(){
	for (int i = 0; i < 20; i++)
		arr[i] = 0;
}

int main()
{
	int tc;
	cin >> tc;
	for (int t = 1; t <= tc; t++) {
		ans = 0;
		cin >> n >> k;
		for (int i = 0; i < n; i++) {
			cin >> arr[i];
		}

		dfs(0, 0);

		cout << "#" << t << " " << ans << endl;

		clearArr();
		ans = 0;
	}

	return 0;
}
