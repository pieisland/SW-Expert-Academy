#include<cstdio>
#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<cstring>
#include<queue>
#include<math.h>
using namespace std;

/*
2072. 홀수만 더하기
그냥. 기본의 기본의 기본 문제.
추천 문제에 떠서 없애려고 풀었다.
*/

int main()
{
	int tc;
	cin >> tc;
	for (int t = 1; t <= tc; t++) {
		int num;
		int ans= 0;
		for (int i = 0; i < 10; i++) {
			cin >> num;
			if (num % 2 == 1) {
				ans += num;
			}
		}
		cout << "#" << t << " " << ans << endl;
	}

	return 0;
}
